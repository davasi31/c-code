using System;

namespace New.netProject
{
    class Program
    {
        static void Main(string[] args)
        {


            Console.WriteLine("Enter your name:");
            string name = Console.ReadLine();

            while (name == "")
            {
                Console.WriteLine("Enter your name: ");
                name = Console.ReadLine();
            }

            Console.WriteLine("Hello" + name);

            Console.ReadKey();

        }
    }
}

